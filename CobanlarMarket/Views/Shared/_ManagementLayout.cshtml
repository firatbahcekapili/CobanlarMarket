    <script>
        jQuery(document).ready(function () {
            jQuery('input[name="dateRange"]').daterangepicker({
                autoUpdateInput: false,
                singleDatePicker: true,
                locale: {
                    cancelLabel: 'Clear'
                }
            });
            jQuery('input[name="dateRange"]').on('apply.daterangepicker', function (ev, picker) {
                jQuery(this).val(picker.startDate.format('MM/DD/YYYY'));
            });
            jQuery('input[name="dateRange"]').on('cancel.daterangepicker', function (ev, picker) {
                jQuery(this).val('');
            });
        });
    </script>
    <script>
        $(document).ready(function() {
            var currentPage = "@currentPage"; // Razor değişkenini JavaScript'e geçiriyoruz

            // Tüm collapse menüleri kontrol et
            $('#sidebar-menu .has-sub').each(function() {
                var $this = $(this);
                var $collapse = $this.find('.collapse');
                var target = $collapse.attr('id');

                // Collapse menüdeki tüm linkleri kontrol et
                $collapse.find('a.sidenav-item-link').each(function() {
                    var href = $(this).attr('href');
                    if (currentPage === href.replace('/', '')) {
                        $this.addClass('active');
                        $(this).closest('li').addClass('active');
                        $collapse.collapse('show'); // Collapse elementini aç
                        $('a[data-target="#' + target + '"]').attr('aria-expanded', 'true'); // Parent link'in durumunu güncelle
                        $('a[data-target="#' + target + '"]').removeClass('collapsed'); // Parent link'teki collapsed sınıfını kaldır
                    }
                });
            });
        });
    </script>


